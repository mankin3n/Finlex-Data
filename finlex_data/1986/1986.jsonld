d"},"temporalVersion":{"@id":"eli:has_member","@type":"@id"},"temporalVersions":{"@id":"eli:has_member","@type":"@id"},"idLocal":"eli:id_local","title_fi":{"@id":"eli:title","@language":"fi"},"title_sv":{"@id":"eli:title","@language":"sv"},"content_fi":{"@id":"sfl:undefined","@language":"fi"},"content_sv":{"@id":"sfl:undefined","@language":"sv"}}}